# IBM MQ Connection Settings for Real MQ Integration
MQ_QUEUE_MANAGER=QM1
MQ_HOST=localhost
MQ_PORT=1414
MQ_CHANNEL=DEV.APP.SVRCONN
MQ_USER_ID=
MQ_PASSWORD=
MQ_TIMEOUT=30000

# Default Queues
MQ_INPUT_QUEUE=DEV.QUEUE.1
MQ_OUTPUT_QUEUE=DEV.QUEUE.RESPONSE
MQ_ERROR_QUEUE=DEV.QUEUE.ERROR

# Logging
MQ_LOG_LEVEL=info
MQ_DEBUG=false

# Connection Pool Settings
MQ_MAX_CONNECTIONS=10
MQ_CONNECTION_RETRY=3
MQ_RETRY_DELAY=5000
